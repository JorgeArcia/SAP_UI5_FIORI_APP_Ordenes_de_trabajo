<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<SelectDialog
		title="Seleccione Categoria" 
		items="{ path: 'MatchCategorias>/'}" 
		search="onSearchCategorias" 
		confirm="_closeValueHelpCategorias"
		cancel="_closeCategorias">
		<StandardListItem title="{MatchCategorias>idCategoria}" info="{MatchCategorias>DescCategoria}"></StandardListItem>
	</SelectDialog>
</core:FragmentDefinition>